{
  "name": "watr",
  "version": "1.3.1",
  "description": "Ligth & fast WAT compiler",
  "main": "watr.js",
  "type": "module",
  "scripts": {
    "test": "node test",
    "build": "rollup src/watr.js --file watr.js --format esm --name \"Watr\"",
    "min": "terser watr.js -o watr.min.js --module -c passes=3 -m"
  },
