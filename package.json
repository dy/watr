    "type": "git",
    "url": "git+https://github.com/audio-lab/watr.git"
  },
  "files": [
    "watr.js",
    "watr.min.js"
  ],
  "keywords": [
    "wat",
    "compiler",
    "wabt"
  ],
  "author": "Dmitry Iv",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/audio-lab/watr/issues"
  },
  "homepage": "https://github.com/audio-lab/watr#readme",
  "devDependencies": {
    "rollup": "^2.70.1",
    "terser": "^5.12.1",
    "tst": "^7.1.1",
    "wabt": "^1.0.28",
    "wat-compiler": "^1.0.0"
  }
}
