{
  "name": "watr",
  "version": "3.2.1",
  "description": "Ligth & fast WAT compiler",
  "main": "watr.js",
  "exports": {
    ".": "./watr.js",
    "./parse": "./src/parse.js",
    "./print": "./src/print.js",
    "./compile": "./src/compile.js"
  },
  "type": "module",
  "scripts": {
    "test": "node test",
    "types": "npx tsc src/*.js watr.js --allowJs --declaration --emitDeclarationOnly --declarationMap --outDir types",
    "postinstall": "git submodule update --init --recursive"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dy/watr.git"
  },
  "files": [
    "src",
    "watr.js",
    "watr.d.ts"
  ],
  "types": "./types/watr.d.ts",
  "typesVersions": {
    "*": {
      "compile": ["./types/compile.d.ts"],
      "compile.js": ["./types/compile.d.ts"],
      "const": ["./types/const.d.ts"],
      "const.js": ["./types/const.d.ts"],
      "parse": ["./types/parse.d.ts"],
      "parse.js": ["./types/parse.d.ts"],
      "print": ["./types/print.d.ts"],
      "print.js": ["./types/print.d.ts"],
      "util": ["./types/util.d.ts"],
      "util.js": ["./types/util.d.ts"],
      "watr": ["./types/watr.d.ts"],
      "watr.js": ["./types/watr.d.ts"]
    }
  },
  "keywords": [
    "wat",
    "wasm",
    "wasm compiler",
    "wat2wasm",
    "wabt",
    "pretty-print",
    "webassembly",
    "wasm-text",
    "wast",
    "wat-compiler"
  ],
  "author": "Dmitry Iv",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dy/watr/issues"
  },
  "homepage": "https://github.com/dy/watr#readme",
  "devDependencies": {
    "tst": "^8.0.2"
  }
}
